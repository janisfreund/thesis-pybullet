<?xml version="1.0"?>
<robot name="floor">
    <material name="grey">
        <color rgba="0.95 0.95 0.95 1.0"/>
    </material>

    <link name="base">
        <contact>
            <lateral_friction value="1"/>
        </contact>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value=".0"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="200 200 0.001"/>
            </geometry>
            <material name="grey"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -1"/>
            <geometry>
                <box size="200 200 1"/>
            </geometry>
        </collision>
    </link>

</robot>